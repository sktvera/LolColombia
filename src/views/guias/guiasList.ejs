<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  

    <!-- fontawesome iconos -->
    <script src="https://kit.fontawesome.com/272d0d9efe.js" crossorigin="anonymous"></script>

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

   <!--  CSS -->
   <link rel="stylesheet" href="css/style_index.css"> 

    <title>LoLColombia</title>
  </head>
  <body>

    <%- include('../partials/header') %>

    
    <main role="main">


  
		<div class="album py-5 bg-light">
		  <div class="container">
  
			<div class="row">
			  <% for(let i = 0; i < datos.length; i ++) { %>
			  <div class="col-md-4">
				<div class="card mb-4 box-shadow">
				  <a href="/guias/guiaCampeon/<%=datos[i].id %>"><img class="img-thumbnail" src="/images/<%=datos[i].imagen %>" alt="<%=datos[i].id %>"> </a>
				  <div class="card-body">
					<h3><%=datos[i].nombre %></h3>
					<p class="card-text">Estadisticas: </p> 
					<ul>
					  <li>tipo: <%=datos[i].tipo %></li>
					  <li>vida: <%=datos[i].vida %></li>
					  <li>mana: <%=datos[i].apellido %></li>
					  <li>regMana: <%=datos[i].regMana %></li>
					  <li>velocidad: <%=datos[i].velocidad %></li>
					  <li>daño: <%=datos[i].daño %></li>
					  <li>velAtaque: <%=datos[i].velAtaque %></li>
					  <li>rango: <%=datos[i].rango %></li>
					  <li>resMagia: <%=datos[i].resMagia %></li>
					</ul>
					<div class="d-flex justify-content-between align-items-center">
					  <div class="btn-group">
						<button type="button" class="btn btn-sm btn-outline-secondary">View</button>
						<button type="button" class="btn btn-sm btn-outline-secondary">Edit</button>
					  </div>
					  <small class="text-muted">9 mins</small>
					</div>
				  </div>
				</div>
			  </div>
			  <% } %>
			</div>
		  </div>
		</div>
  
	  </main>

    <%- include('../partials/footer') %>
    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  </body>
</html>